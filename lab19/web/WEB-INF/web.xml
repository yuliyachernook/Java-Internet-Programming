<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <display-name>Lab16,17</display-name>

    <servlet>
        <servlet-name>Ccc</servlet-name>
        <servlet-class>classes.Ccc</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Ccc</servlet-name>
        <url-pattern>/Ccc</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Ccc1</servlet-name>
        <servlet-class>classes.Ccc1</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Ccc1</servlet-name>
        <url-pattern>/Ccc1</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Ccc2</servlet-name>
        <servlet-class>classes.Ccc2</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Ccc2</servlet-name>
        <url-pattern>/Ccc2</url-pattern>
    </servlet-mapping>

    <filter>
        <filter-name>F1</filter-name>
        <filter-class>classes.filters.F1</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>F1</filter-name>
        <url-pattern>/Ccc</url-pattern>
    </filter-mapping>

    <filter>
        <filter-name>F2</filter-name>
        <filter-class>classes.filters.F2</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>F2</filter-name>
        <url-pattern>/Ccc.jsp</url-pattern>
    </filter-mapping>
    <filter-mapping>
        <filter-name>F2</filter-name>
        <url-pattern>/Ccc</url-pattern>
    </filter-mapping>
    <filter>
        <filter-name>F3</filter-name>
        <filter-class>classes.filters.F3</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>F3</filter-name>
        <url-pattern>/Ccc</url-pattern>
    </filter-mapping>

    <listener>
        <listener-class>classes.filters.L1</listener-class>
    </listener>
    <listener>
        <listener-class>classes.filters.L2</listener-class>
    </listener>
</web-app>